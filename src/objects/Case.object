    <validationRules>
        <fullName>Action_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Action__c,&apos;Infopass&apos;), ISBLANK(ActionSummary__c))</errorConditionFormula>
        <errorMessage>Please add Infopass summary.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VIP_Customer_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR( 
ISPICKVAL( Account.AccountRating__c , &apos;4&apos;), 
ISPICKVAL( Account.AccountRating__c , &apos;5&apos;) 
), 
NOT(VIPCustomer__c)
)</errorConditionFormula>
        <errorMessage>The associated Account is a VIP Customer</errorMessage>
    </validationRules>
