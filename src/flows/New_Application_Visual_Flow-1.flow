            <isRequired>true</isRequired>
            <validationRule>
                <errorMessage>HEEEEYYYY</errorMessage>
                <formulaExpression>Len({!Receipt_Number}) &lt;= 50 || NOT(Len({!Receipt_Number}) &lt; 1)</formulaExpression>
